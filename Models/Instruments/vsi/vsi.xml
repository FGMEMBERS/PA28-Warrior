<?xml version="1.0"?>

<PropertyList>

	<path>vsi.ac</path>

	<animation>
		<type>scale</type>
		<x-offset>0.95</x-offset>
		<y-offset>0.95</y-offset>
		<z-offset>0.95</z-offset>
	</animation>

	<animation>
		<type>material</type>
		<object-name>Face</object-name>
		<object-name>PointerCentre</object-name>
		<object-name>PointerTip</object-name>
		<emission>
			<red>0.6</red>
			<green>0.6</green>
			<blue>0.6</blue>
			<factor-prop>controls/lighting/panel-norm</factor-prop>
		</emission>
	</animation>

	<animation>
		<type>rotate</type>
		<object-name>PointerCentre</object-name>
		<object-name>PointerTip</object-name>
		<property>instrumentation/vertical-speed-indicator/indicated-speed-fpm-actual</property>
		<interpolation>
			<entry><ind>-2000</ind><dep>-176</dep></entry>
			<entry><ind>-1500</ind><dep>-133</dep></entry>
			<entry><ind>-1000</ind><dep> -88</dep></entry>
			<entry><ind> -500</ind><dep> -44</dep></entry>
			<entry><ind>    0</ind><dep>   0</dep></entry>
			<entry><ind>  500</ind><dep>  44</dep></entry>
			<entry><ind> 1000</ind><dep>  88</dep></entry>
			<entry><ind> 1500</ind><dep> 133</dep></entry>
			<entry><ind> 2000</ind><dep> 176</dep></entry>
		</interpolation>
		<axis>
			<x>-1</x>
			<y>0</y>
			<z>0</z>
		</axis>
		<center>
			<x-m>0</x-m>
			<y-m>0</y-m>
			<z-m>0</z-m>
		</center>
	</animation>

</PropertyList>
