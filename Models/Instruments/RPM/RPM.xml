<?xml version="1.0"?>

<PropertyList>
	
	<path>RPM.ac</path>
	
	<animation>
		<type>material</type>
		<object-name>face</object-name>
		<object-name>needle</object-name>
		<object-name>hour.01</object-name>
		<object-name>hour.1</object-name>
		<object-name>hour.10</object-name>
		<object-name>hour.100</object-name>
		<object-name>hour.1000</object-name>
		<emission>
			<red>0.6</red>
			<green>0.6</green>
			<blue>0.6</blue>
			<factor-prop>controls/lighting/panel-norm</factor-prop>
		</emission>
	</animation>
	
	<animation>
		<type>rotate</type>
		<object-name>needle</object-name>
		<property>engines/engine[0]/rpm</property>
		<interpolation>
			<entry><ind> 274</ind><dep>  0.000</dep></entry>
			<entry><ind> 300</ind><dep>  2.150</dep></entry>
			<entry><ind>3500</ind><dep>272.675</dep></entry>
		</interpolation>
		<axis>
			<x>-1</x>
			<y>0</y>
			<z>0</z>
		</axis>
		<center>
			<x-m>0</x-m>
			<y-m>0</y-m>
			<z-m>0</z-m>
		</center>
	</animation>

</PropertyList>
