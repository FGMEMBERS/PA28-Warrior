<?xml version="1.0"?>

<PropertyList>
	
	<path>RPM.ac</path>
	
	<animation>
		<type>material</type>
		<object-name>Face</object-name>
		<object-name>PointerTip</object-name>
		<emission>
			<red>0.6</red>
			<green>0.6</green>
			<blue>0.6</blue>
			<factor-prop>sim/model/material/instruments/factor</factor-prop>
		</emission>
	</animation>
	
	<animation>
		<type>rotate</type>
		<object-name>needle</object-name>
		<property>engines/engine[0]/rpm</property>
		<interpolation>
			<entry><ind>   0</ind><dep> 0</dep></entry>
			<entry><ind> 300</ind><dep> 2.15</dep></entry>
			<entry><ind>3500</ind><dep> 272.675</dep></entry>
		</interpolation>
		<axis>
			<x>-1</x>
			<y>0</y>
			<z>0</z>
		</axis>
		<center>
			<x-m>0</x-m>
			<y-m>0</y-m>
			<z-m>0</z-m>
		</center>
	</animation>

</PropertyList>





