<PropertyList>
	
	<path>SwitchPanel.ac</path>
	
	<animation>
		<type>material</type>
		<emission>
			<red>0.1</red>
			<green>0.1</green>
			<blue>0.1</blue>
			<factor-prop>controls/lighting/panel-norm</factor-prop>
		</emission>
	</animation>
	
	<animation>
		<type>rotate</type>
		<object-name>NavDimmer</object-name>
		<property>controls/switches/nav-lights-factor</property>
		<factor>-90</factor>
		<axis>
			<x>-0.00578</x>
			<y>1.0</y>
			<z>0.0</z>
		</axis>
		<center>
			<x-m>-0.00578</x-m>
			<y-m>0.0</y-m>
			<z-m>0.0</z-m>
		</center>
	</animation>
	
	<animation>
		<type>pick</type>
		<object-name>NavDimmer</object-name>
		<action>
			<button>0</button>
			<button>3</button>
			<repeatable type="bool">false</repeatable>
			<binding>
				<command>property-adjust</command>
				<property>controls/switches/nav-lights-factor</property>
				<step>0.2</step>
				<min>0</min>
				<max>1</max>
			</binding>
		</action>
		<action>
			<button>1</button>
			<button>4</button>
			<repeatable type="bool">false</repeatable>
			<binding>
				<command>property-adjust</command>
				<property>controls/switches/nav-lights-factor</property>
				<step>-0.2</step>
				<min>0</min>
				<max>1</max>
			</binding>
		</action>
	</animation>

	<animation>
		<type>rotate</type>
		<object-name>BAT.sw</object-name>
		<property>controls/electrical/battery</property>
		<factor>30</factor>
		<axis>
			<x>0.0</x>
			<y>-1.0</y>
			<z>0.0</z>
		</axis>
		<center>
			<x-m>0.0</x-m>
			<y-m>0.0</y-m>
			<z-m>-0.0</z-m>
		</center>
	</animation>
	
	<animation>
		<type>pick</type>
		<object-name>BAT.sw</object-name>
		<action>
			<button>0</button>
			<repeatable type="bool">false</repeatable>
			<binding>
				<command>property-toggle</command>
				<property>controls/electrical/battery</property>
			</binding>
			<binding>
				<command>nasal</command>
				<script>setprop("/sim/sounde/switch2", 1);</script>
			</binding>
		</action>
	</animation>

