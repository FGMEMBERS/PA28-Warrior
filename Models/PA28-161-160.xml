		<object-name>gear.high.L</object-name>
		<object-name>gear.low.scissor.L</object-name>
		<object-name>gear.high.scissor.L</object-name>
		<object-name>gear.fairing.L</object-name>
	</animation>
	<animation>
		<name>RightGear</name>
