	<!--Annunciator/Indicator Panel-->
	<animation>
		<type>pick</type>
		<object-name>annun.test.btn</object-name>
		<action>
			<button>0</button>
			<repeatable type="bool">false</repeatable>
			<binding>
				<command>property-toggle</command>
				<property>controls/switches/annun-test</property>
			</binding>
			<binding>
				<command>property-assign</command>
				<property>controls/switches/annun-test-btn</property>
				<value>1</value>
			</binding>
			<mod-up>
				<binding>
					<command>property-assign</command>
					<property>controls/switches/annun-test-btn</property>
					<value>0</value>
				</binding>
			</mod-up>
		</action>
	</animation>
