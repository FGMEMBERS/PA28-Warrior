	<input>
		<keyboard>
			<key n="12">
				<name>Ctrl-L</name>
				<desc>toggle panel lights</desc>
				<binding>
					<command>property-cycle</command>
					<property>/controls/switches/panel-lights-factor</property>
					<value>1</value>
					<value>0</value>
				</binding>
			</key>
			<key n="24">
				<name>Ctrl-x</name>
				<desc>Reset zoom to default</desc>
				<binding>
					<command>nasal</command>
					<script>libraries.fovZoom(0);</script>
				</binding>
			</key>
			<key n="49">
				<name>1</name>
				<desc>Elevator trim up</desc>
				<repeatable>true</repeatable>
				<binding>
					<command>nasal</command>
					<script>controls.elevatorTrim(-1);</script>
				</binding>
				<mod-shift>
					<desc>Look back left</desc>
					<binding>
						<command>property-assign</command>
						<property>/sim/current-view/goal-heading-offset-deg</property>
						<property>/sim/view/config/back-left-direction-deg</property>
					</binding>
				</mod-shift>
			</key>
			<key n="55">
				<name>7</name>
				<desc>Elevator trim down</desc>
				<repeatable>true</repeatable>
				<binding>
					<command>nasal</command>
					<script>controls.elevatorTrim(1);</script>
				</binding>
				<mod-shift>
					<desc>Look front left</desc>
					<binding>
						<command>property-assign</command>
						<property>/sim/current-view/goal-heading-offset-deg</property>
						<property>/sim/view/config/front-left-direction-deg</property>
					</binding>
				</mod-shift>
			</key>
			<key n="66">
				<name>B</name>
				<desc>Toggle parking brake on or off</desc>
				<binding>
					<command>nasal</command>
					<script>controls.applyParkingBrake(1)</script>
				</binding>
				<mod-up>
					<binding>
						<command>nasal</command>
						<script>controls.applyParkingBrake(0)</script>
					</binding>
				</mod-up>	
			</key>
			<key n="68">
				<name>D</name>
				<desc>Autopilot Disconnect</desc>
				<binding>
					<command>property-assign</command>
					<property>/it-stec55x/input/disc</property>
					<value>1</value>
				</binding>
				<binding>
					<command>nasal</command>
					<script>stec55x.button.DISC();</script>
				</binding>
				<mod-up>
					<binding>
						<command>property-assign</command>
						<property>/it-stec55x/input/disc</property>
						<value>0</value>
					</binding>
				</mod-up>
			</key>
			<key n="88">
				<name>X</name>
				<desc>Increase field of view</desc>
				<repeatable type="bool">true</repeatable>
				<binding>
					<command>nasal</command>
					<script>libraries.fovZoom(1);</script>
				</binding>
			</key>
			<key n="99">
				<name>c</name>
				<desc>S-TEC 55X CWS Button</desc>
				<binding>
					<command>nasal</command>
					<script>stec55x.button.CWS(1);</script>
				</binding>
				<mod-up>
					<binding>
						<command>nasal</command>
						<script>stec55x.button.CWS(0);</script>
					</binding>
				</mod-up>
			</key>
			<key n="101">
				<name>e</name>
				<desc>Thrust Lever Idle</desc>
				<binding>
					<command>nasal</command>
					<script>
					libraries.doIdleThrust();
					</script>
				</binding>
			</key>
			<key n="102">
				<name>f</name>
				<desc>Thrust Lever Full</desc>
				<binding>
					<command>nasal</command>
					<script>
					libraries.doFullThrust();
					</script>
				</binding>
			</key>
			<key n="106">
				<name>j</name>
				<desc>toggle tanks</desc>
				<binding>
					<command>property-cycle</command>
					<property>/systems/fuel/selected-tank</property>
					<value>1</value>
					<value>2</value>
				</binding>
			</key>
			<key n="115">
				<name>s</name>
				<desc>Kill starter-key</desc>
				<binding>
					<command>nasal</command>
					<script>setprop("/sim/unused/nothing", 0);</script>
				</binding>
			</key>
			<key n="120">
				<name>x</name>
				<desc>Decrease field of view</desc>
				<repeatable type="bool">true</repeatable>
				<binding>
					<command>nasal</command>
					<script>libraries.fovZoom(-1);</script>
				</binding>
			</key>
		</keyboard>
		<mice n="0">
			<mouse n="0">
				<mode n="0">
					<button n="3">
						<binding n="0">
							<script></script>
						</binding>
						<binding n="1">
							<condition>
								<and>
									<property>/devices/status/mice/mouse[0]/button[2]</property>
